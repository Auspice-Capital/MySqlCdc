<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <Nullable>enable</Nullable>
    <PackageId>MySqlCdc</PackageId>
    <Version>3.0.0-rc.2</Version>
    <Authors>Ruslan Ulianets</Authors>
    <PackageTags>MySQL;MariaDB;Binlog;CDC;Change Data Capture;Replication;Streaming</PackageTags>
    <Description>MySQL/MariaDB binlog change data capture (CDC) connector for .NET</Description>
    <PackageProjectUrl>https://github.com/rusuly/MySqlCdc</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageReleaseNotes>
This version targets net6.0
    </PackageReleaseNotes>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.IO.Pipelines" Version="6.0.0-rc.2.21480.5" />
    <PackageReference Include="System.Text.Json" Version="6.0.0-rc.2.21480.5" />
  </ItemGroup>
</Project>